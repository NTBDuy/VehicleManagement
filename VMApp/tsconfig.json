{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",

    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@/types/*": ["./types/*"],
      "@/components/*": ["./components/*"],
      "@/config/*": ["./config/*"],
      "@/contexts/*": ["./contexts/*"],
      "@/utils/*": ["./utils/*"],
      "@/assets/*": ["./assets/*"]
    }
  }
}
